
public class program {
	
	public static void main(String[] args) {
		
		int [] compassDirections = {0, 1, 2, 3};
		
		String [] directions = {"R5", "L5", "R5", "R3"};
		int x = 0;
		int y = 0;
		int facing = compassDirections[0];
		
		for (String direction : directions) {
			char turn = direction.charAt(0);
			int distance = Integer.parseInt(direction.substring(1));
			if (turn == 'R') {
				facing = compassDirections[(facing + 1) % compassDirections.length];
			} else {
				facing = compassDirections[(facing - 1) % compassDirections.length];
			}
			
			switch (facing) {
			case 0:
				y += distance;
				break;
			case 1:
				x += distance;
				break;
			case 2:
				y -= distance;
				break;
			case 3:
				x -= distance;
				break;
			}
		}
		
		System.out.println((x+y) + " blocks");
		

	}


}
